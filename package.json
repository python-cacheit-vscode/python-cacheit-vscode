{
  "name": "python-cacheit",
  "displayName": "python-cacheit",
  "description": "Extension to inject caching snippets into python code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:python-cacheit.helloWorld",
    "onCommand:python-cacheit.cacheit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-cacheit.helloWorld",
        "title": "Hello World ad"
      },
      {
        "command": "python-cacheit.cacheit",
        "title": "cacheit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "10.x",
    "nanoid": "^3.3.3",
    "typescript": "^4.6.4"
  }
}
